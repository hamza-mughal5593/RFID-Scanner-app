<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp" >
 
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="30dp" >

        <View
            android:id="@+id/View04"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:background="#BFEFFF" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

         <TextView
        android:id="@+id/TextView_antpck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tab4_atpw_TextView_antpck"
        android:textColor="#ff1493"
        android:textSize="18sp" />
           
        </LinearLayout>
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <RadioGroup
            android:id="@+id/radioGroup_antcheck"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <RadioButton
                android:id="@+id/radio_tb4nck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:text="@string/tab4_atpw_radio_tb4nck" />

            <RadioButton
                android:id="@+id/radio_tb4ck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tab4_atpw_radio_tb4ck" />
        </RadioGroup>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/button_checkantget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramget" />

        <Button
            android:id="@+id/button_checkantset"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramset" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView_tab4pw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#BFEFFF"
        android:text="@string/tab4_atpw_textView_tab4pw"
        android:textColor="#ff1493"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/textView_ant1_pwr"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_ant1_pwr" />

            <Spinner
                android:id="@+id/spinner_ant1rpow"
                android:layout_width="95dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView_tab4wp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_tab4wp" />

            <Spinner
                android:id="@+id/spinner_ant1wpow"
                android:layout_width="95dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView_tb4a2rp"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_TextView_tb4a2rp" />

            <Spinner
                android:id="@+id/spinner_ant2rpow"
                android:layout_width="95dp"
                android:layout_height="38dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/TextView07"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_tab4wp" />

            <Spinner
                android:id="@+id/spinner_ant2wpow"
                android:layout_width="95dp"
                android:layout_height="38dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView_tb4a3rp"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_tb4a3rp" />

            <Spinner
                android:id="@+id/spinner_ant3rpow"
                android:layout_width="95dp"
                android:layout_height="38dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/TextView05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_tab4wp" />

            <Spinner
                android:id="@+id/spinner_ant3wpow"
                android:layout_width="95dp"
                android:layout_height="38dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView_tba4rp"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_tba4rp" />

            <Spinner
                android:id="@+id/spinner_ant4rpow"
                android:layout_width="95dp"
                android:layout_height="38dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/TextView03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tab4_atpw_textView_tab4wp" />

            <Spinner
                android:id="@+id/spinner_ant4wpow"
                android:layout_width="95dp"
                android:layout_height="38dp"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/button_antpowget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramget" />

        <Button
            android:id="@+id/button_antpowset"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramset" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/TextView_tba16allrp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/tab4_atpw_textView_tab4pw" />

        <EditText
            android:id="@+id/editText_allpw"
            android:layout_width="97dp"
            android:layout_height="wrap_content"
            android:text="2000" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </EditText>

        <Button
            android:id="@+id/button_setallsamepw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/tab4_atpw_button_allset" />
    </LinearLayout>

</LinearLayout>