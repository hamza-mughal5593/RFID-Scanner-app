<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    
        <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="30dp" >

        <View
            android:id="@+id/View04"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:background="#BFEFFF" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
        android:id="@+id/TextView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Target"
        android:textColor="#ff1493"
        android:textSize="18sp" />
           
        </LinearLayout>
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Spinner
            android:id="@+id/spinner_bluetarget"
            android:layout_width="89dp"
            android:layout_height="44dp"
            android:layout_weight="1" />

        <Button
            android:id="@+id/button_blue_target"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramget" />

        <Button
            android:id="@+id/button_blue_targetset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramset" />
    </LinearLayout>

    <TextView
        android:id="@+id/TextView_blqv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#BFEFFF"
        android:text="@string/tab_blueset_gen2_TextView_blqv"
        android:textColor="#ff1493"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Spinner
            android:id="@+id/spinner_bluegen2q"
            android:layout_width="89dp"
            android:layout_height="44dp"
            android:layout_weight="1" />

        <Button
            android:id="@+id/button_blue_gen2qget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramget" />

        <Button
            android:id="@+id/button_blue_gen2qset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramset" />
    </LinearLayout>

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#BFEFFF"
        android:text="Session"
        android:textColor="#ff1493"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Spinner
            android:id="@+id/spinner_bluegen2session"
            android:layout_width="89dp"
            android:layout_height="44dp"
            android:layout_weight="1" />

        <Button
            android:id="@+id/button_blue_gen2sesget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramget" />

        <Button
            android:id="@+id/button_blue_gen2sesset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/str_paramset" />
    </LinearLayout>

</LinearLayout>